package michael_fournier_COSC101_calculator;
import java.util.Scanner;

public class Calculator {
	public static void inputGathering() {
		Scanner scanner = new Scanner(System.in);
		char operationChoice = ' ';
		System.out.print("Enter an operation (+, -, *, /, %(mod)): ");
		operationChoice = scanner.next().charAt(0);
		scanner.close();
	}
	

	public static void main(String[] args) {
		System.out.println("Michael Fournier COSC101");
		System.out.println("This is my calculator project");
		inputGathering();
	}
}
